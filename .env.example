# Supabase Configuration
# Get your access token from: https://supabase.com/dashboard/account/tokens
SUPABASE_TOKEN=your_supabase_access_token_here
SUPABASE_PROJECT_ID=your_project_id_here
SUPABASE_PROJECT_URL=https://your-project-id.supabase.co
SUPABASE_API_KEY_SECRET=your_supabase_api_key_here

# Database URLs (optional, for direct database access)
DATABASE_URL=postgresql://postgres.your-project-id:password@aws-1-eu-west-1.pooler.supabase.com:6543/postgres?pgbouncer=true
DIRECT_URL=postgresql://postgres.your-project-id:password@aws-1-eu-west-1.pooler.supabase.com:5432/postgres

# Application URLs
SITE_URL=http://localhost:5173
VITE_API_URL=http://localhost:3000/api
CORS_ORIGIN=*

# API Configuration
API_PORT=3000

# Environment
ENVIRONMENT=development

